public class Linear_search {

  public static void main(String[] args) {
    int[] data = { 4, 2, 6, 80, 33 };
    int tar = 33;
    int flag = 0;
    for (int i = 0; i < data.length; i++) {
      if (data[i] == tar) {
        System.out.print("position is : " + i);
        flag = 1;
        break;
      }
    }
    if (flag == 0) {
      System.out.print("element is not present ");
    }

  }

}
